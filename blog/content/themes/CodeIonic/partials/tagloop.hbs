

    <div class="postExcerpt">
        <div class="list">
            {{! This is the post loop - each post will be output using this markup }}
            {{#foreach posts}}
                <div class="item row">
                    <div class="summary">
                      <a href="{{url}}">
                        <h3>
                          {{{title}}}
                        </h3>
                        <p class="subhead">
                          {{date format="DD MMMM YYYY"}}
                          |
                          {{tags prefix=" tagged "}}
                        </p>
                      </a>
                    </div>

                </div>
            {{/foreach}}
        </div>
    </div>

{{! Previous/next page links - displayed on every page }}
{{pagination}}
